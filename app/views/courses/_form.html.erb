<%= simple_form_for @course do |f| %>
  <%= f.input :title %>
  <%= f.input :level %>
  <%= f.association :classroom, label_method: :room, value_method: :id, include_blank: false %>
  <%= f.input :start_date %>
  <%= f.input :end_date %>
  <%= f.input :start_time %>
  <%= f.input :end_time %>
  <%=  f.input :description %>
  <%= f.button :submit %>
<% end %>

