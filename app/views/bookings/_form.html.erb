<%= simple_form_for @booking do |f| %>
  <%= f.association :course, label_method: :title, value_method: :id, include_blank: false %>
  <%= f.input :level %>
  <%= f.input :start_date %>
  <%= f.input :end_date %>
  <%= f.input :start_time %>
  <%= f.input :end_time %>
  <%=  f.input :description %>
  <%= f.button :submit %>
<% end %>